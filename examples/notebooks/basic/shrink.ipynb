    "\"\"\"Shrink the triangulation of a mesh to make the inside visible.\"\"\"\n",
    "\n",
    "from vedo import *\n",
    "embedWindow('ipyvtk') # or k3d or ipygany\n",
    "\n",
    "pot = load(dataurl+\"teapot.vtk\").shrink(0.75)\n",
